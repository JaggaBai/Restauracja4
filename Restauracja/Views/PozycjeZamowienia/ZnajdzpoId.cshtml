@model PozycjaZamowienia

@{ ViewData["Title"] = "Usuń"; }
<div class="row text">
    <div class="col-md-8 offset-2">
        <p>
            <h1>Do usunięcia</h1>
        </p>
        <div class="row">
            <div class="col-md-8 offset-2">
                    <div class="form-group">
                        <label asp-for="Nazwa" class="control-label"></label>
                        <input asp-for="Nazwa" class="form-control" />
                        <span asp-validation-for="Nazwa" class="text-danger"></span>

                    </div>
                    <div class="form-group">
                        <a class=" btn btn-outline-primary" asp-action="Index">Powrót</a>
                        <a class=" btn btn-outline-primary" asp-action="Zmień" asp-route-id="@Model.IdPozycji"></a>
                    </div>
            </div>
        </div>
    </div>
</div>